<Bonfire.UI.Common.PageHeaderLive>
  <:breadcrumbs>
    <Bonfire.Classify.Web.BreadcrumbsItemsLive category={@category} />
  </:breadcrumbs>

  <:right_action>
    <StatelessComponent
      module={maybe_component(Bonfire.UI.Boundaries.BoundaryIconStatelessLive)}
      with_label
      id={"boundary_#{id(@category)}"}
      object_boundary={@object_boundary}
      object={@category}
      object_type={Types.object_type(@category)}
    />
  </:right_action>
</Bonfire.UI.Common.PageHeaderLive>
